            <value id="GTK_SOURCE_DRAW_SPACES_LEADING" name="Leading"></value>
            <value id="GTK_SOURCE_DRAW_SPACES_TEXT" name="Text"></value>
            <value id="GTK_SOURCE_DRAW_SPACES_TRAILING" name="Trailing"></value>
            <value id="GTK_SOURCE_DRAW_SPACES_ALL" name="All"></value>
